#include "Application.h"

SDL_Window* m_window;

int main(int argc, char** argv) {
    if(enet_initialize() != 0) {
        SDL_ShowSimpleMessageBox(SDL_MESSAGEBOX_ERROR, "Fatal Error!", "Enet has failed to initialize!", m_window);
        exit(-1);
    }
    
    Application app;
    app.EventLoop();    
        

    enet_deinitialize();
    return 0;
}